{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/inventory/(.*)": "<rootDir>/../libs/inventory/src/$1",
    "@app/inventory": "<rootDir>/../libs/inventory/src",
    "@app/quests/(.*)": "<rootDir>/../libs/quests/src/$1",
    "@app/quests": "<rootDir>/../libs/quests/src",
    "@app/account/(.*)": "<rootDir>/../libs/account/src/$1",
    "@app/account": "<rootDir>/../libs/account/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/character/(.*)": "<rootDir>/../libs/character/src/$1",
    "@app/character": "<rootDir>/../libs/character/src"
  }
}