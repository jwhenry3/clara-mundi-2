{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/node-backend/src",
  "monorepo": true,
  "root": "apps/node-backend",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/node-backend/tsconfig.app.json"
  },
  "projects": {
    "node-backend": {
      "type": "application",
      "root": "apps/node-backend",
      "entryFile": "main",
      "sourceRoot": "apps/node-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/node-backend/tsconfig.app.json"
      }
    },
    "master-server": {
      "type": "application",
      "root": "apps/master-server",
      "entryFile": "main",
      "sourceRoot": "apps/master-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/master-server/tsconfig.app.json"
      }
    },
    "chat-server": {
      "type": "application",
      "root": "apps/chat-server",
      "entryFile": "main",
      "sourceRoot": "apps/chat-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-server/tsconfig.app.json"
      }
    },
    "quest-server": {
      "type": "application",
      "root": "apps/quest-server",
      "entryFile": "main",
      "sourceRoot": "apps/quest-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/quest-server/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    }
  }
}